#pragma config(Sensor, dgtl1,  SONAR_VAL,      sensorSONAR_inch)
#pragma config(Sensor, dgtl3,  WHISKER_LEFT,   sensorTouch)
#pragma config(Sensor, dgtl4,  WHISKER_RIGHT,  sensorTouch)
#pragma config(Sensor, dgtl5,  ENCODER_RIGHT,  sensorQuadEncoder)
#pragma config(Sensor, dgtl7,  ENCODER_LEFT,   sensorQuadEncoder)
#pragma config(Sensor, dgtl9,  BUTTON,         sensorTouch)
#pragma config(Motor,  port3,           RIGHT,         tmotorVex393, openLoop, reversed)
#pragma config(Motor,  port4,           LEFT,          tmotorVex393, openLoop)
//*!!Code automatically generated by 'ROBOTC' configuration wizard               !!*//

#include "Header.h"
task main()
{


while(true){
		if (WHISKER_LEFT == true && WHISKER_RIGHT == true){
		  STOP();
		  }

		else if (WHISKER_LEFT == false && WHISKER_RIGHT == false){
  		FORWARD_IN();
		}


		if (sensorValue[SONAR_VAL] >= 10){
			motor[LEFT] = 50;
			motor[RIGHT] = 50;
			}
    else{
			motor[LEFT] = 0;
			motor[RIGHT] = 0;
		}

	}
	}
